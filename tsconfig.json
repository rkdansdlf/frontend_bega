{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "skipLibCheck": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "types": ["vite/client", "react"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@radix-ui/react-*": ["node_modules/@radix-ui/react-*"],
      "lucide-react@*": ["node_modules/lucide-react"],
      "embla-carousel-react@*": ["node_modules/embla-carousel-react"],
      "cmdk@*": ["node_modules/cmdk"],
      "input-otp@*": ["node_modules/input-otp"],
      "react-day-picker@*": ["node_modules/react-day-picker"],
      "react-hook-form@*": ["node_modules/react-hook-form"],
      "react-resizable-panels@*": ["node_modules/react-resizable-panels"],
      "recharts@*": ["node_modules/recharts"],
      "sonner@*": ["node_modules/sonner"],
      "vaul@*": ["node_modules/vaul"],
      "figma:asset/*": ["src/assets/*"]
    }
  },
  "include": ["src"]
}
